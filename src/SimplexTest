# import Pkg
# Pkg.add("JuMP")
using JuMP
include("Simplex.jl")

model = Model()

@variable(model, 0 <= x1 )
@variable(model, 0 <= x2 )

@objective(model, Max, 340*x1 + 404*x2 )

@constraint(model, 420x1 + 760x2 <= 5480.0)
@constraint(model, 9x1 + 5x2 <= 61.0)

showModel(model)
solve(model)
